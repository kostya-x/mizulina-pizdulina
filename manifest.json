{
  "manifest_version": 1,
  "name": "Pizdulina",
  "version": "1.0",
  "description": "Заменяет на всех страницах слово 'пиздолин' на 'пиздолин'",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": []
}